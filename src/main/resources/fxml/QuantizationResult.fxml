<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #FCF6FF;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.QuantizationResultController">
   <children>
      <ImageView fx:id="imageView" fitHeight="400.0" fitWidth="400.0" layoutX="200.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="490.0" layoutY="499.0" mnemonicParsing="false" onAction="#save" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #9900cc;" text="Save" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="200.0" layoutY="499.0" mnemonicParsing="false" onAction="#goBack" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #9900cc;" text="Go Back" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="347.0" layoutY="499.0" mnemonicParsing="false" onAction="#IndexedImage" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #9900cc;" text="Indexed Image" textAlignment="CENTER" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
